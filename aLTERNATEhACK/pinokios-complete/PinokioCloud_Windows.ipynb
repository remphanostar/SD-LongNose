    "#@title Launch Streamlit UI with ngrok Tunnel\n",
    "from pyngrok import ngrok\n",
    "import threading\n",
    "import time\n",
    "import psutil\n",
    "\n",
    "def kill_streamlit_windows():\n",
    "    \"\"\"Kill Streamlit processes on Windows\"\"\"\n",
    "    killed = False\n",
    "    for proc in psutil.process_iter(['pid', 'name', 'cmdline']):\n",
    "        try:\n",
    "            if proc.info['name'] and 'python' in proc.info['name'].lower():\n",
    "                cmdline = proc.info.get('cmdline', [])\n",
    "                if cmdline and any('streamlit' in str(cmd) for cmd in cmdline):\n",
    "                    print(f'  Killing Streamlit process {proc.info[\"pid\"]}')\n",
    "                    proc.terminate()\n",
    "                    killed = True\n",
    "        except (psutil.NoSuchProcess, psutil.AccessDenied):\n",
    "            pass\n",
    "    return killed\n",
    "\n",
    "# Kill any existing Streamlit processes\n",
    "print('üõë Stopping any existing Streamlit processes...')\n",
    "if kill_streamlit_windows():\n",
    "    print('  ‚úÖ Existing processes stopped')\n",
    "    time.sleep(2)\n",
    "else:\n",
    "    print('  ‚úÖ No existing processes found')\n",
    "\n",
    "# Configure Streamlit for headless operation\n",
    "os.environ['STREAMLIT_SERVER_HEADLESS'] = 'true'\n",
    "os.environ['STREAMLIT_SERVER_PORT'] = '8501'\n",
    "os.environ['STREAMLIT_BROWSER_GATHER_USAGE_STATS'] = 'false'\n",
    "\n",
    "print('\\nüöÄ Starting Streamlit server...')\n",
    "\n",
    "# Start Streamlit in background using Windows runner\n",
    "def run_streamlit():\n",
    "    try:\n",
    "        subprocess.run([\n",
    "            sys.executable, '-m', 'streamlit', 'run',\n",
    "            str(runner_path),\n",
    "            '--server.port', '8501',\n",
    "            '--server.address', '0.0.0.0',\n",
    "            '--server.headless', 'true',\n",
    "            '--browser.gatherUsageStats', 'false',\n",
    "            '--theme.base', 'light'\n",
    "        ], cwd=repo_dir, shell=True)\n",
    "    except Exception as e:\n",
    "        print(f'Error starting Streamlit: {e}')\n",
    "\n",
    "# Start in thread\n",
    "thread = threading.Thread(target=run_streamlit, daemon=True)\n",
    "thread.start()\n",
    "\n",
    "# Wait for server to start\n",
    "print('‚è≥ Waiting for server to start...')\n",
    "time.sleep(8)  # Extra time on Windows\n",
    "\n",
    "# Create ngrok tunnel\n",
    "print('\\nüåê Creating public tunnel...')\n",
    "try:\n",
    "    public_url = ngrok.connect(8501, \"http\")\n",
    "    \n",
    "    print('\\n' + '='*60)\n",
    "    print('‚úÖ PinokioCloud Streamlit UI is running!')\n",
    "    print('='*60)\n",
    "    print(f'\\nüîó Public URL: {public_url}')\n",
    "    print(f'üè† Local URL: http://localhost:8501')\n",
    "    print('\\nüì± Open either URL in your browser to access the UI')\n",
    "    print('\\nThe Streamlit app provides:')\n",
    "    print('  ‚Ä¢ App search and filtering')\n",
    "    print('  ‚Ä¢ One-click install/run/stop')\n",
    "    print('  ‚Ä¢ Real-time status monitoring')\n",
    "    print('  ‚Ä¢ Beautiful modern interface')\n",
    "    print('\\n‚ö†Ô∏è  Keep this cell running to maintain the server')\n",
    "    \n",
    "except Exception as e:\n",
    "    print(f'‚ùå Failed to create tunnel: {e}')\n",
    "    print('\\nBut you can still access locally at: http://localhost:8501')"
   ]
