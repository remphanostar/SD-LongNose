    "#@title Create Windows-Compatible Streamlit Runner\n",
    "# Create a wrapper script to run Streamlit with proper Windows configuration\n",
    "streamlit_script = f'''\n",
    "import streamlit as st\n",
    "import sys\n",
    "import os\n",
    "from pathlib import Path\n",
    "\n",
    "# Set paths for Windows\n",
    "repo_path = Path(r\"{repo_dir}\")\n",
    "sys.path.insert(0, str(repo_path))\n",
    "os.chdir(repo_path)\n",
    "\n",
    "# Import and run the FIXED Streamlit app\n",
    "from ui.streamlit_app_fixed import main\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()\n",
    "'''\n",
    "\n",
    "# Write the runner script\n",
    "runner_path = repo_dir / 'run_streamlit_windows.py'\n",
    "with open(runner_path, 'w', encoding='utf-8') as f:\n",
    "    f.write(streamlit_script)\n",
    "\n",
    "print('‚úÖ Windows Streamlit runner script created')\n",
    "print(f'   Path: {runner_path}')"
   ]
